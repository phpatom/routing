    "type": "library",
    "require": {
        "nikic/fast-route": "^1.3",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "fig/http-message-util": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Atom\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atom\\Routing\\Test\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
